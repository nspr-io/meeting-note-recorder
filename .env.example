# Google Calendar OAuth Credentials
# Get these from https://console.cloud.google.com
GOOGLE_CLIENT_ID=your_actual_client_id_here.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your_actual_client_secret_here

# Recall.ai API Configuration
RECALL_API_KEY=your_recall_api_key_here
RECALL_API_URL=https://us-east-1.recall.ai

# Logging Configuration
# Options: error, warn, info, debug
# error = only errors in console
# warn = errors + warnings in console
# info = errors + warnings + info in console (default for dev)
# debug = all logs in console (very verbose)
LOG_LEVEL=warn